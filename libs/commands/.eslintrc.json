{
    "extends": ["../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "rules": {}
        },
        {
            "files": ["*.ts", "*.tsx"],
            "rules": {
                "@typescript-eslint/ban-types": 0,
                "@typescript-eslint/no-empty-interface": 0
            }
        },
        {
            "files": ["*.js", "*.jsx"],
            "rules": {}
        }
    ]
}
